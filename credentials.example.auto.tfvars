# This file is an example of the required variables for this Terraform project.
# To use it, rename this file to "credentials.auto.tfvars" and replace the
# placeholder values with your actual Civo and Cloudflare credentials.

# Your Civo API token.
civo_token = "<civo-private-token>"

# Your Cloudflare account email address.
cloudflare_email = "<cloudflare-email>"

# Your Cloudflare Global API Key.
cloudflare_api_key = "<cloudflare-api-key>"